{
  "name": "brave-dave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "set:aws": "bash scripts/set-aws-credentials.sh",
    "local": "pnpm set:aws && sst dev",
