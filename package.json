    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sst": "^3.17.0",
    "tailwindest": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/aws-lambda": "^8.10.0",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "aws-cdk-lib": "^2.0.0",
    "concurrently": "^9.1.2",
    "constructs": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
